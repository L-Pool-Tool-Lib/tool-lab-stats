{
  "name": "tool-lab-stats",
  "version": "1.0.0",
  "private": true,
  "description": "tool-lab-stats",
  "author": "Damien Rembert",
  "type": "module",
  "keywords": [
    "gatsby"
  ],
  "scripts": {
    "get-user": "bash scripts/get_user.sh",
    "get-data": "bash scripts/query_myturn.sh",
    "merge-data": "bash scripts/merge_myturn_data.sh",
    "clean-data": "python3 scripts/clean_data.py",
    "clean-data-gender": "python3 scripts/clean_data_gender.py",
    "remove-data": "rm -f data/*.csv; rm -f data/*.json; rm -f data/*.gz",
    "develop": "gatsby develop",
    "start": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "chart.js": "4.4.9",
    "gatsby": "5.14.3",
    "gatsby-plugin-react-leaflet": "4.0.3",
    "gatsby-source-filesystem": "5.14.0",
    "gatsby-transformer-csv": "5.14.0",
    "leaflet": "1.9.3",
    "react": "18.2.0",
    "react-chartjs-2": "5.3.0",
    "react-dom": "18.2.0",
    "react-leaflet": "4.2.1"
  },
  "devDependencies": {
    "@eslint/js": "9.27.0",
    "@playwright/test": "1.57.0",
    "@types/node": "20.11.19",
    "@types/react": "18.2.55",
    "@types/react-dom": "18.2.19",
    "dotenv": "17.2.3",
    "eslint": "9.27.0",
    "typescript": "5.3.3",
    "typescript-eslint": "8.32.1"
  }
